{
  "type": "project",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-sockets": "*",
    "bobv/irker-utils": "^1.0",
    "symfony/console": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/property-access": "^6.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
